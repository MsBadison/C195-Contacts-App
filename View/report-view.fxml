<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="App.Controller.ReportController">
   <children>
      <Button fx:id="reportCloseButton" layoutX="724.0" layoutY="552.0" mnemonicParsing="false" onAction="#reportClose" text="Close" />
      <TextArea fx:id="reportText" editable="false" layoutX="62.0" layoutY="320.0" prefHeight="175.0" prefWidth="676.0">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </TextArea>
      <ComboBox fx:id="reportMonth" layoutX="250.0" layoutY="69.0" prefHeight="25.0" prefWidth="150.0" promptText="Select Month" />
      <ComboBox fx:id="reportType" layoutX="406.0" layoutY="69.0" prefWidth="150.0" promptText="Select Type" />
      <TextField fx:id="reportCountBox" editable="false" layoutX="622.0" layoutY="69.0" prefHeight="25.0" prefWidth="116.0" />
      <Text layoutX="62.0" layoutY="86.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Appointments by time and month" />
      <Button fx:id="timeMonthRunButton" layoutX="569.0" layoutY="69.0" mnemonicParsing="false" onAction="#timeMonthRun" text="Run" />
      <Text layoutX="62.0" layoutY="267.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Appointments by contact" />
      <ComboBox fx:id="reportContact" layoutX="250.0" layoutY="250.0" prefWidth="150.0" promptText="Select Contact" />
      <Button fx:id="contactRunButton" layoutX="421.0" layoutY="250.0" mnemonicParsing="false" onAction="#contactRun" text="Run" />
      <Text layoutX="62.0" layoutY="176.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Appointments by location" />
      <ComboBox fx:id="reportLocation" layoutX="250.0" layoutY="159.0" prefWidth="150.0" promptText="Select Location" />
      <Button fx:id="locationRunButton" layoutX="412.0" layoutY="159.0" mnemonicParsing="false" onAction="#locationRun" text="Run" />
      <TextField fx:id="reportLocationBox" editable="false" layoutX="464.0" layoutY="159.0" prefHeight="25.0" prefWidth="116.0" />
   </children>
</AnchorPane>
